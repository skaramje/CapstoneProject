    "paths": {
        "/account/all": {
            "get": {
                "tags": ["Accounts"],
                "summary": "Get all accounts in system",
                "responses": {
                    "200": {
                        "description": "OK"                        
                    }
                }
            }
        },
        "/account/create": {
            "get": {
                "tags": ["Accounts"],
                "description": "Create new acount in system",
                "parameters": [
                    {
                        "name": "account",
                        "email": "body",
                        "uid": "uid"
                        
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "New account is created"
                    }
                }
            }
        }
    }
